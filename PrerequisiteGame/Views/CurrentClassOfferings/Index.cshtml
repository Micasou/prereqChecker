@model IEnumerable<PrerequisiteGame.Models.CurrentClassOffering>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.ClassOffering.CourseCode)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Quarter)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Year)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.timeOffered)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.ClassOffering.CourseCode)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Quarter)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Year)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.timeOffered)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.CurrentClassOfferingID }) |
            @Html.ActionLink("Details", "Details", new { id=item.CurrentClassOfferingID }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.CurrentClassOfferingID })
        </td>
    </tr>
}

</table>
